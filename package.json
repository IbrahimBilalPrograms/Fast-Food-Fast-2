{
  "name": "Fast-Food-Fast",
  "version": "1.0.0",
  "description": "Fast-Food_Fast is a food delivery service for a restaurant",
  "main": "server.js",
  "repository": "https://github.com/eltNEG/Fast-Food-Fast.git",
  "author": "eltNEG <yorewole@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "coveralls": "^3.0.2",
    "express": "^4.16.3"
  },
  "scripts": {
    "devstart": "nodemon --exec npm run babel-node -- ./server.js",
    "babel-node": "babel-node --presets=env",
    "startdev": "npm run babel-node server.js",
    "start": "npm run build && node dist/server.js",
    "build": "rm -rf dist && babel ./ -d dist --ignore node_modules,ui",
    "eslint": "./node_modules/.bin/eslint",
    "test-init": "node node_modules/jasmine/bin/jasmine init",
    "test": "./node_modules/.bin/jasmine --config=./spec/support/jasmine.json",
    "coverage": "istanbul cover node_modules/jasmine/bin/jasmine.js --config=./spec/support/jasmine.json --captureExceptions spec/ && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "istanbul": "^0.4.5",
    "jasmine": "^3.2.0",
    "jasmine-node": "^1.16.0",
    "request": "^2.88.0"
  }
}
